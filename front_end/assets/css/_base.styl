.contain
  width 1024px
  margin 0 auto
  padding 0 1%
  transition width 500ms ease-in
  @media screen and (min-width: 1500px)
    width 65%

.cl
  clear both

.right
  float right

.left
  float left

.list-inline
  li
    list-style-type none
    display inline-block

.padding-reset
  padding 0 !important
