@import '_variables'

b
  font-family font-family-primary-bold
  font-weight normal

.btn
  color white
  cursor pointer
  display inline-block
  padding 7px 11px
  border none
  font-size font-size-base
  font-family font-family-bold

.btn-group
  margin: 5px 0
  .btn
    margin-right: 5px

.tag
  padding: 4px 7px
  border: solid 1px #111
  border-radius: 3px
  display: inline-block
  font-size: 11px
  margin: 1px
  cursor: pointer

a.active
  font-weight: bold
