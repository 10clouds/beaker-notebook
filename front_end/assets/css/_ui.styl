.btn
  cursor: pointer
  display: inline-block
  padding: 7px 11px
  border: solid 2px #111;
  font-size: 14px;
  font-weight: bold;

.btn-group
  margin: 5px 0
  .btn
    margin-right: 5px

.tag
  padding: 4px 7px
  border: solid 1px #111
  border-radius: 3px
  display: inline-block
  font-size: 11px
  margin: 1px
  cursor: pointer
