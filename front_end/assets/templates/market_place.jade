h1 bunsen market place
treecontrol.tree-classic.col-quarter(
   tree-model="treeData",
   options="treeOptions"
   on-selection="onTreeSelection(node)")

  item: {{node.category}} - ({{node.count}})

.col-half
  input(type="text", ng-model="filterBy", placeholder="Search for Data")
  ul
    li(ng-repeat="item in data | filter:filterBy ")
      a(ui-sref="marketItem({id: item.id})") {{item.title}}
      p.description {{item.description | limitTo:100}}...
.cl
