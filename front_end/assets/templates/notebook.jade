.notebook
  .sidebar-right
    .publishing.sidebar-box.light
      h3 Publishing
      .content
        a.view-published(ng-if="published" ui-sref="publications.items.item({id: notebook.publication.id})") View published version &raquo;
        a.publish(ng-if="!published" ng-click="openPublishModal()") Publish this notebook &raquo;

  .main-content
    a.back-to-project.btn.btn-light.left(ui-sref="projects.items.item({id: project.id})") Back to project home
    button.btn.btn-light.right.close-notebook(close-notebook="{{ notebook.id }}") close notebook
    button.btn.btn-light.right.rename(rename-notebook-toggle ng-click="showRenameModal()") Rename
    .cl
    br br
    h2 Notebook:&nbsp;
      span.name {{notebook.name}}
    beakernotebook(notebook-id="{{ notebook.id }}", src="{{ notebookLocation }}", width="600", height="1000")
