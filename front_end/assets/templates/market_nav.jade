input.search(ng-model="marketPlace.searchTerm", placeholder="Search")

treecontrol.tree-classic(
   tree-model="treeData",
   options="treeOptions"
   on-selection="onTreeSelection(node)")

  item: {{node.category}} - ({{node.count}})

br
.label-pair
  label
    b Related Tags
  br
  dl.related-tags
    dt.related-tag(ng-repeat="tag in marketPlace.relatedTags")
      a.tag-name(ng-click="searchByTag(tag)" href='') {{tag.name}}
      span(ng-show='isTagSelected(tag)') *
