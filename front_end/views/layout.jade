!!!
html(ng-app="bunsen")
  head
    meta(charset='utf8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
    link(rel='stylesheet', href='css/master.css')
    script(src="js/vendor.js", type="text/javascript")
    script(src="js/templates.js", type="text/javascript")
    script(src="js/main.js", type="text/javascript")
    - if (process.env['API_BASE_URL'] !== void 0)
      script.
        window.API_BASE_URL= "#{process.env['API_BASE_URL']}"

    - if (process.env['NODE_ENV']=="test")
      script(src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js", type="text/javascript")

  body
    .application(ng-controller="application")
      include _app_header
      nav.main-nav
        .contain
          ul.main-nav-list.list-inline
            li
              a.nav-item.projects(ng-class='$state.includes("projects.**") && "nav-item-active"', ui-sref="projects.items") My Projects
            li
              a.nav-item.market-place(ui-sref-active='nav-item-active', ui-sref="marketPlace.items") Marketplace
          .cl
      .contain
        #root(ui-view="root")
          != content
