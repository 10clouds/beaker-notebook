{
  "app": {
    "command": "--migrate --watch --delay=5",
    "links": [
      "db",
      "provisioner",
      "elasticsearch"
    ],
    "ports": "3000:3000",
    "volumes": [
      "/vagrant/app:/var/app"
    ],
    "environment": [
      "NODE_ENV=development",
      "HOSTNAME=localhost:7777",
      "MANDRILL_PASSWORD=-QG6MBSWNPR7sPwRali8Jg",
      "MANDRILL_USER=ops+mandrill@mojotech.com",
      "MAIL_FROM=ops+bunsen@mojotech.com"
    ]
  },
  "web": {
    "command": "--watch",
    "ports": "8090:7777",
    "build": "front_end",
    "volumes": [
      "/vagrant/front_end:/var/www"
    ]
  },
  "db": {
    "command": "-r",
    "ports": "5432:5432",
    "build": "postgres",
    "volumes": [
      "/var/bunsen_dbs:/var/lib/postgresql/9.3/main",
      "/vagrant/postgres:/var/postgres"
    ]
  },
  "beaker": {
    "ports": "8801:8801"
  },
  "provisioner": {
    "ports": "3001:3001",
    "volumes": [
      "/var/run/docker.sock:/var/run/docker.sock"
    ],
    "command": ""
  },
  "gateway": {
    "ports": "7777:80",
    "links": [
      "app",
      "web"
    ],
    "volumes": [
      "/dev/log:/dev/log",
      "/var/run/docker.sock:/var/run/docker.sock",
      "/vagrant/gateway:/opt/gateway"
    ],
    "command": "beaker localhost:7777"
  },
  "elasticsearch": {
    "ports": "9200:9200",
    "volumes": [
      "/var/elasticsearch/data:/var/elasticsearch/data"
    ]
  },
  "data_catalog_mock": {
    "ports": "1880:80",
    "volumes": [
      "/vagrant/data_catalog_mock:/var/www"
    ]
  }
}
