            "input": {
                "body": [
                    "var ch = new beaker.Crosshair({color: beaker.prefs.theme.plotColors[0], width: 2, style: beaker.StrokeType.DOT})",
                    "var plot = new beaker.Plot({crosshair: ch});",
                    "var y1 = [4, 8, 16, 20, 32]",
                    "var base = [2, 4, 8, 10, 16];",
                    "var ss = [beaker.StrokeType.SOLID, ",
                    "          beaker.StrokeType.SOLID, ",
                    "          beaker.StrokeType.DASH, ",
                    "          beaker.StrokeType.DOT, ",
                    "          beaker.StrokeType.DASHDOT, ",
                    "          beaker.StrokeType.LONGDASH]",
                    "plot.add(new beaker.Area({y: y1, base: base, color: beaker.prefs.theme.plotColors[2]}))",
                    "plot.add(new beaker.Stems({y: y1, base: base, style: ss, width: 5, color: beaker.prefs.theme.plotColors[1]}))"
                ]
