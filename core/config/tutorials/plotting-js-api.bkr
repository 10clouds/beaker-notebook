            "type": "code",
            "evaluator": "JavaScript",
            "input": {
                "body": [
                    "var x = [1, 4, 6, 8, 10]",
                    "var y = [3, 6, 4, 5, 9]",
                    "var pp = new beaker.Plot({title: 'Bars, Lines, Points and 2nd yAxis', ",
                    "                          xLabel: \"xLabel\", ",
                    "                          yLabel: \"yLabel\", ",
                    "                          omitCheckboxes: true})",
                    "pp.add(new beaker.YAxis({label: \"Right yAxis\"}))",
                    "pp.add(new beaker.Bars({displayName: \"Bar\", ",
                    "                        x: [1,3,5,7,10], ",
                    "                        y: [100, 120,90,100,80], ",
                    "                        width: 1}))",
                    "pp.add(new beaker.Line({displayName: \"Line\", ",
                    "                        x: x, ",
                    "                        y: y, ",
                    "                        width: 6, ",
                    "                        yAxis: \"Right yAxis\"}))",
                    "pp.add(new beaker.Points({x: x, ",
                    "                          y: y, ",
                    "                          size: 10, ",
                    "                          shape: beaker.ShapeType.DIAMOND,",
                    "                          yAxis: \"Right yAxis\"}))"
                ]
            },
